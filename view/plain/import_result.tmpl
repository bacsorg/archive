<% c++ #include <bacs/archive/web/content/form.hpp> %>
<% skin plain %>
    <% view import_result_result uses bacs::archive::web::content::import_map_result extends import_map_result %>
        <% template caption() %>
            <% gt "Problem import result" %>
        <% end template %>
    <% end view %>
    <% view import_result uses bacs::archive::web::content::import_result extends base_form %>
        <% template scripts() %>
            <% include base_form::scripts() %>
            <script src="/static/plain/protobuf.js" type="text/javascript"></script>
        <% end template %>
        <% template subtitle() %>
            <% gt "Import results" %>
        <% end template %>
        <% template form() %>
            <h3><% gt "Get import results" %></h3>
            <form method="post" action="">
                <% form as_p form %>
            </form>
        <% end template %>
        <% template result() %>
            <% if result %>
                <% using import_result_result with *result as result_view %>
                    <% include render() from result_view %>
                <% end using %>
            <% end %>
        <% end template %>
    <% end view %>
<% end skin %>
